<div>
  <h1>State #show</h1>
  <h3>State: <%= String.upcase(@name) %></h3>

  <div>
    <h3>Senators</h3>
    <%= for senator <- @senators do %>
      <a href="<%= WhoWeb.RoutesHelper.member_id_path(senator["id"]) %>">
        <p>
          <%= WhoWeb.MemberView.parse_member_name(senator) %>
        </p>
      </a>
    <% end %>
  </div>

  <div>
    <h3>Representatives</h3>
    <%= for rep <- @reps do %>
      <a href="<%= WhoWeb.RoutesHelper.member_id_path(rep["id"]) %>">
        <p>
          <%= WhoWeb.MemberView.parse_member_name(rep) %>
        </p>
      </a>
    <% end %>
  </div>

</div>
